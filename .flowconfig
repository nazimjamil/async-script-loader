[ignore]

[include]

[libs]
<PROJECT_ROOT>/flow-typed
<PROJECT_ROOT>/node_modules/**/*.js.flow

[lints]

[options]
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnoreLine
emoji=true
module.system.node.resolve_dirname=node_modules

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import

[untyped]
.*/node_modules/**/.*
